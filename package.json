{
  "name": "ng-brazil",
  "version": "0.5.7",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "cpr dist/ng-brazil src/ng-brazil && ng build --app packages && rimraf src/ng-brazil",
    "build:lib": "rimraf dist && ng-packagr -p ng-brazil/package.json",
    "publish": "npm run build:lib && cd dist/ng-brazil && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "kill": "kill $(lsof -t -i:4200)"
  },
  "dependencies": {
    "@angular/common": "6.0.0-beta.5",
    "@angular/compiler": "6.0.0-beta.5",
    "@angular/core": "6.0.0-beta.5",
    "@angular/forms": "6.0.0-beta.5",
    "@angular/http": "6.0.0-beta.5",
    "@angular/platform-browser": "6.0.0-beta.5",
    "@angular/platform-browser-dynamic": "6.0.0-beta.5",
    "@angular/router": "6.0.0-beta.5",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^0.4.0",
    "@angular/cli": "~1.7.0",
    "@angular/compiler-cli": "6.0.0-beta.5",
    "@angular/language-service": "6.0.0-beta.5",
    "@types/jasmine": "~2.8.0",
    "@types/node": "~9.4.0",
    "codelyzer": "~4.2.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "2.1.0",
    "protractor": "~5.3.0",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.0",
    "tsickle": "^0.27.1",
    "typescript": "2.5.3"
  }
}
