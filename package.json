{
  "name": "ng-brazil",
  "version": "2.4.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "dist": "npm run build:app && rimraf src/ng-brazil",
    "build:lib": "rimraf dist && ng-packagr -p ng-brazil/package.json",
    "build:sass": "npm rebuild sass",
    "publishnpm": "npm run build:lib && cd dist/ng-brazil && npm publish",
    "test": "npm run build:lib && npm run dist && npm run e2e",
    "copy": "mkdir -p src/ng-brazil/ && cp -R dist/ng-brazil/* src/ng-brazil",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "dependencies": {
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "autoprefixer": "^10.2.3",
    "core-js": "^2.4.1",
    "js-brasil": ">=2.4.0",
    "postcss": "^8.2.4",
    "rxjs-compat": "^6.0.0",
    "sass": "^1.32.5",
    "tailwindcss": "^2.0.3",
    "text-mask-core": "^5.1.2",
    "tslib": "^2.0.0",
    "typescript": "~4.0.2",
    "webpack": "~4.11.1",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular-devkit/schematics": "^11.2.2",
    "@angular/cli": "^11.2.2",
    "@angular/compiler-cli": "^11.2.3",
    "@angular/language-service": "^11.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "~9.4.0",
    "cpr": "^3.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "11.2.4",
    "protractor": "^7.0.0",
    "rimraf": "^2.6.1",
    "ts-node": "~5.0.0",
    "tsickle": "^0.36.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.2"
  }
}
